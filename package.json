{
  "name": "view-in-chrome",
  "displayName": "View in Chrome",
  "description": "Open html files in Chrome",
  "version": "0.0.1",
  "publisher": "lina",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "view",
    "open",
    "chrome",
    "open in chrome",
    "preview in chrome",
    "browser",
    "html",
    "preview html",
    "open html"
  ],
  "activationEvents": [
    "onCommand:extension.openChrome"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openChrome",
        "title": "View In Chrome"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "opn": "^4.0.2"
  }
}
